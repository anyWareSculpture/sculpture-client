{
  "name": "@anyware/sculpture-client",
  "version": "1.8.4",
  "description": "The embedded system code for the sculpture itself",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "npm run build-dev && npm run launch && npm run build-dev -- --watch",
    "build": "NODE_ENV=production npm run webpack -- -p",
    "build-dev": "npm run webpack --",
    "webpack": "webpack --config webpack.config.dev.js",
    "launch": "chrome --load-and-launch-app=$PWD/build",
    "package": "mkdir -p dist && cd build && zip -r ../dist/anyware-sculpture.zip * && cd -",
    "lint": "eslint --ext=.js,.jsx app",
    "test": "npm run lint",
    "serial-emulator": "babel-node --stage 0 utilities/serial-emulator.js",
    "svg": "svgo -f ./app/src/react/svg/status-icons -o out && svg-sprite-generate -d out -o ./app/src/react/svg/status-sprites.svg",
    "anyware": "./scripts/anyware"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anyWareSculpture/sculpture-client.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anyWareSculpture/sculpture-client/issues"
  },
  "homepage": "https://github.com/anyWareSculpture/sculpture-client#readme",
  "dependencies": {
    "@anyware/sound-assets": "^1.3.0",
    "anyware": "*",
    "babel-polyfill": "^6.9.1",
    "browser-serialport": "^2.1.0",
    "chrome": "^0.1.0",
    "flux": "^2.0.3",
    "gulp": "^3.9.1",
    "howler": "^1.1.26",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babelify": "^7.3.0",
    "bestzip": "^1.1.3",
    "blessed": "^0.1.81",
    "browserify": "^13.1.0",
    "child-process-promise": "^2.2.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^6.4.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-shell": "^0.4.2",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-zip": "^3.0.2",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "react-loader": "^2.4.0",
    "react-svg-loader": "^1.1.1",
    "run-sequence": "^1.1.0",
    "serialport": "^4.0.7",
    "style-loader": "^0.13.1",
    "svg-sprite-generator": "0.0.7",
    "svgo": "^0.7.2",
    "through2": "^2.0.0",
    "url-loader": "^0.5.7",
    "username": "^2.3.0",
    "webpack": "^2.2.0"
  }
}
